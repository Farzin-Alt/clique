#include <iostream>

using namespace std;

void change(double &a, double &b){
    double temp;
    temp = a;
    a = b;
    b = temp;
}

int main()
{
    double a = 8.0, b = 10.0;

    cout << "A: " << a << " B: " << b << endl;
    change(a, b);
    cout << "New A: " << a << " New B: " << b;

    return 0;
}
