#include <iostream>

using namespace std;

int main()
{

    int n;
    cout << "Enter N: ";
    cin >> n;

    int *arr = new int[n];
    for(int i = 0, j = 1; i < n; i++, j += 2){
        arr[i] = j;
    }

    for(int i = 0; i < n; i++){
        cout << "Element " << i << ": " << arr[i] << endl;
    }

    return 0;
}
