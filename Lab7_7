#include <iostream>

using namespace std;

int main()
{

    int a, b, c;

    cout << "Enter sides of triangle: ";
    cin >> a >> b >> c;

    cout << "The right of saying is ";
    if(a <= 0 || b <= 0 || c <= 0 || a + b <= c || a + c <= b || b + c <= a)
        cout << "false";
    else cout << "true";

    return 0;
}
