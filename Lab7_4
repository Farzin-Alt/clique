#include <iostream>

using namespace std;

int main()
{

    int a, nums[3];
    cout << "Enter number: ";
    cin >> a;
    nums[2] = a % 10; nums[1] = (a / 10) % 10; nums[0] = a / 100;

    cout << "The right of saying is ";
    if(nums[2] > nums[1] && nums[1] > nums[0]) cout << "true";
    else if(nums[2] < nums[1] && nums[1] < nums[0]) cout << "true";
    else cout << "false";

    return 0;
}
