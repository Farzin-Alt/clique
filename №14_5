
#include <iostream>

using namespace std;

int main()
{

    int n, i, j;
    cout << "Enter size: ";
    cin >> n;

    int *arr = new int[n];

    for(int i = 0; i < n; i++){
        cout << "Enter element: ";
        cin >> arr[i];
    }

    for(i = 0; i < n - 1; i++){
        for(j = i + 1; j < n; j++)
            if(arr[i] == arr[j])
                break;
        if(arr[i] = arr[j]) break;
    }

    cout << "Equals elements: " << i << ' ' << j;

    return 0;
}
